@inherits RazorTemplateBase
<h1>@Model.Context["namespace"]</h1>

@Html.RenderPartial(Templates.Summary)
@Html.RenderPartial(Templates.Remarks)

<h1>Remarks</h1>
<p>@Model.Remarks</p>

<ul>
@foreach(var type in @Model.Source.XPathSelectElements("//class")) {
        <li>@type.Attribute("name").Value</li>
}
</ul>